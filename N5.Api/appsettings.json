{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "N5db": "Server=tcp:mealserver.database.windows.net,1433;Initial Catalog=N5db;Persist Security Info=False;User ID=mealdbadmin;Password=G2ma8lhcf05**;MultipleActiveResultSets=False;Encrypt=True;TrustServerCertificate=False;Connection Timeout=30;"
